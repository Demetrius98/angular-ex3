        template: tslib__WEBPACK_IMPORTED_MODULE_0__["__importDefault"](__webpack_require__(/*! raw-loader!./build-chat.component.html */ "./node_modules/raw-loader/dist/cjs.js!./src/app/build-chat/build-chat.component.html")).default,
        styles: [tslib__WEBPACK_IMPORTED_MODULE_0__["__importDefault"](__webpack_require__(/*! ./build-chat.component.css */ "./src/app/build-chat/build-chat.component.css")).default]
    })
