        this._auth.loginUser(this.loginUserData)
            .subscribe(res => {
            console.log(res);
            localStorage.setItem('token', res.token);
            this._messengerService.refreshData();
            this._router.navigate(['/']);
        }, err => {
            console.log(err);
            alert(err.error);
        });
    }
    navigateToRegister() {
        this._router.navigate(['/register']);
    }
};
